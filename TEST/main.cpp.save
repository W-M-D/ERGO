#include <iostream>
#include <forward_list>
  using namespace std;

int main()
{
    int counter = 0 ;
    forward_list <int> data_list ;
    while(1)
    {
      
    }
    auto it = data_list.before_begin();
    for(int i =0; i < 10; i++)
    {
      data_list.emplace_after(it,counter);
      counter++;
      it++;
    }
    if(counter >=200)
    {
      while(!data_list.empty())
      {
        cout << data_list.front() << endl;
        data_list.pop_front();
      }
          return 0;
    }
}
